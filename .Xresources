URxvt*termName: rxvt-256color
URxvt*font: xft:hack:size=12, \
            -wuncon-siji-medium-r-normal--10-100-75-75-c-80-iso10646-1, \
            xft:unifont:pixelsize=10
URxvt.perl-ext-common: default,tabbedex,clipboard
URxvt.scrollBar: false

URxvt.underlineURLs: True

URxvt.tabbedex.new-button: False
URxvt.tabbedex.autohide: True
URxvt.tabbedex.no-tabbedex-keys: True
URxvt.tabbedex.tabbar-bg: 15
URxvt.tabbedex.tabbar-fg: 5

URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0

URxvt.modifier: alt

URxvt*iso14755: False
URxvt*iso14755_52: False

URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D

URxvt.keysym.Shift-Down: tabbedex:new_tab
URxvt.keysym.Shift-Up: tabbedex:rename_tab
URxvt.keysym.Shift-Left: tabbedex:prev_tab
URxvt.keysym.Shift-Right: tabbedex:next_tab

URxvt.clipboard.autocopy: true

URxvt.keysym.Control-Shift-C: clipboard:copy
URxvt.keysym.Shift-Control-V: clipboard:paste


*foreground: #a3a3a3
*background: #1f2022

*color0:#1f2022
*color8: #585858

*color1: #f2241f
*color9: #f2241f

*color2: #67b11d
*color10: #67b11d

*color3: #b1951d
*color11: #b1951d

*color4: #4f97d7
*color12: #4f97d7

*color5: #a31db1
*color13: #a31db1

*color6: #2d9574
*color14: #2d9574

*color7: #a3a3a3
*color15: #f8f8f8

